<%= form_for @dish do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :description %>
  <%= f.text_area :description %>
  <%= f.hidden_field :event_id, :value => params[:id] %>
  <%= f.hidden_field :user_id, :value => current_user.id %>
  <%= f.submit "Create Your Dish!" %>
<% end %>
